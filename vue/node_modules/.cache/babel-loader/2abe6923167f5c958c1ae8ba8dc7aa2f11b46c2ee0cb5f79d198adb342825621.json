t>\r\n        </el-form-item>\r\n        <el-form-item label=\"供应材料\" prop=\"things\">\r\n          <el-input v-model=\"form.things\" placeholder=\"供应材料\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"支付期限\" prop=\"payment\">\r\n          <el-select v-model=\"form.payment\" placeholder=\"请选择支付期限\" style=\"width: 200px; margin-left: 5px\">\r\n            <el-option label=\"现金\" value=\"现金\"></el-option>\r\n            <el-option label=\"一周一结\" value=\"一周一结\"></el-option>\r\n            <el-option label=\"半月一结\" value=\"半月一结\"></el-option>\r\n            <el-option label=\"一月一结\" value=\"一月一结\"></el-option>\r\n            <el-option label=\"三月一结\" value=\"三月一结\"></el-option>\r\n            <el-option label=\"半年一结\" value=\"半年一结\"></el-option>\r\n            <el-option label=\"一年一结\" value=\"一年一结\"></el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"状态\" prop=\"status\">\r\n          <el-select style=\"width: 100%\" v-model=\"form.status\">\r\n            <el-option v-for=\"item in ['可用', '不可用']\" :key=\"item\" :value=\"item\" :label=\"item\"></el-option>\r\n          </el-select>\r\n\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"fromVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Notice\",\r\n  data() {\r\n    return {\r\n      tableData: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 10,  // 每页显示的个数\r\n      total: 0,\r\n      name: null,\r\n      phone: null,\r\n      fromVisible: false,\r\n      form: {},\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      rules: {\r\n        name: [\r\n          {required: true, message: '请输入名称', trigger: 'blur'},\r\n        ],\r\n        user: [\r\n          {required: true, message: '请输入联系人', trigger: 'blur'},\r\n        ],\r\n        phone: [\r\n          {required: true, message: '请输入联系人电话', trigger: 'blur'},\r\n        ]\r\n      },\r\n      ids: []\r\n    }\r\n  },\r\n  created() {\r\n    this.load(1)\r\n  },\r\n  methods: {\r\n    handleAdd() {   // 新增数据\r\n      this.form = {}  // 新增数据的时候清空数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    handleEdit(row) {   // 编辑数据\r\n      this.form = JSON.parse(JSON.stringify(row))  // 给form对象赋值  注意要深拷贝数据\r\n      this.fromVisible = true   // 打开弹窗\r\n    },\r\n    save() {   // 保存按钮触发的逻辑  它会触发新增或者更新\r\n      this.$refs.formRef.validate((valid) => {\r\n        if (valid) {\r\n          this.$request({\r\n            url: this.form.id ? '/supplier/update' : '/supplier/add',\r\n            method: this.form.id ? 'PUT' : 'POST',\r\n            data: this.form\r\n          }).then(res => {\r\n            if (res.code === '200') {  // 表示成功保存\r\n              this.$message.success('保存成功')\r\n              this.load(1)\r\n              this.fromVisible = false\r\n            } else {\r\n              this.$message.error(res.msg)  // 弹出错误的信息\r\n            }\r\n          })\r\n        }\r\n      })\r\n    },\r\n    del(id) {   // 单个删除\r\n      this.$confirm('您确定删除吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/supplier/delete/' + id).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    handleSelectionChange(rows) {   // 当前选中的所有的行数据\r\n      this.ids = rows.map(v => v.id)   //  [1,2]\r\n    },\r\n    delBatch() {   // 批量删除\r\n      if (!this.ids.length) {\r\n        this.$message.warning('请选择数据')\r\n        return\r\n      }\r\n      this.$confirm('您确定批量删除这些数据吗？', '确认删除', {type: \"warning\"}).then(response => {\r\n        this.$request.delete('/supplier/delete/batch', {data: this.ids}).then(res => {\r\n          if (res.code === '200') {   // 表示操作成功\r\n            this.$message.success('操作成功')\r\n            this.load(1)\r\n          } else {\r\n            this.$message.error(res.msg)  // 弹出错误的信息\r\n          }\r\n        })\r\n      }).catch(() => {\r\n      })\r\n    },\r\n    load(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/supplier/selectPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n          phone: this.phone,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data?.list\r\n        this.total = res.data?.total\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.phone = null\r\n      this.load(1)\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.load(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n<script setup>\r\n</script>"],"mappings":"AAkGA,MAAAA,WAAA,GAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MAAG;MAChBC,OAAO,EAAE,CAAC;MAAI;MACdC,QAAQ,EAAE,EAAE;MAAG;MACfC,KAAK,EAAE,CAAC;MACRL,IAAI,EAAE,IAAI;MACVM,KAAK,EAAE,IAAI;MACXC,WAAW,EAAE,KAAK;MAClBC,IAAI,EAAE,CAAC,CAAC;MACRC,IAAI,EAAEC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,SAAS,CAAC,IAAI,IAAI,CAAC;MACzDC,KAAK,EAAE;QACLd,IAAI,EAAE,CACJ;UAACe,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,OAAO;UAAEC,OAAO,EAAE;QAAM,CAAC,CACpD;QACDR,IAAI,EAAE,CACJ;UAACM,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,QAAQ;UAAEC,OAAO,EAAE;QAAM,CAAC,CACrD;QACDX,KAAK,EAAE,CACL;UAACS,QAAQ,EAAE,IAAI;UAAEC,OAAO,EAAE,UAAU;UAAEC,OAAO,EAAE;QAAM,CAAC;MAE1D,CAAC;MACDC,GAAG,EAAE;IACP,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,IAAI,CAAC,CAAC,CAAC;EACd,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MAAI;MACd,IAAI,CAACd,IAAI,GAAG,CAAC,CAAC,EAAE;MAChB,IAAI,CAACD,WAAW,GAAG,IAAI,EAAG;IAC5B,CAAC;